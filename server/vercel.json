{
  "version": 2,
  "builds": [{ "src": "./index.js", "use": "@vercel/node" }],
  "routes": [{ "src": "./(.*)", "dest": "/" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "https://code-editor-client.vercel.app",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
    }
  ]
}
